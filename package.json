{
  "name": "rad-dashboard",
  "version": "0.1.0",
  "description": "This project is bootstrapped with [Create Elm App](https://github.com/halfzebra/create-elm-app).",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "devDependencies": {
    "create-elm-app": "^5.22.0",
    "elm": "^0.19.1-6",
    "elm-live": "^4.0.0-rc.1"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "elm make src/Main.elm --optimize --output=elm.js",
    "postbuild": "uglifyjs elm.js --compress pure_funcs=[F2,F3,F4,F5,F6,F7,F8,F9,A2,A3,A4,A5,A6,A7,A8,A9],pure_getters,keep_fargs=false,unsafe_comps,unsafe | uglifyjs --mangle --output public/elm.min.js",
    "start": "elm-live src/Main.elm"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tubo/rad-dashboard.git"
  },
